<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE cases SYSTEM "../../../../cases.dtd" [
        <!ENTITY % content SYSTEM "../../../../Engine/Entities/definitions.inc">
        %content; ]>
<cases>
    <case name="14e_IncorrectEnumValue.def">
        &CLEAN_LOGS;
        &ENGINE_UP;

        <!--# @Receive message with incorrect value (out of range or not part of valid list of enumerated values) for a particular field identifier (tag number).-->
        <transport className="InitiatorSocketTransport" host="localhost" port="3000">

            <!--# logon message and response-->

            <send>8=FIX.4.0#9=123#35=A#34=1#49=TW#52=20031027-14:29:11#56=ISLD#98=0#108=5#10=000#</send>
            <expect>8=FIX\.4\.0#9=\d{1,3}#35=A#34=1#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#98=0#108=5#10=\d{3}#
            </expect>

            <!--#========================-->

            <!--# begin message exchange-->

            <!--#========================-->

            <!--#New order message with incorrect enum value. Handling instructions (21) = 4-->

            <send>
                8=FIX.4.0#9=123#35=D#34=2#49=TW#52=20031027-14:29:11#56=ISLD#11=ID#21=4#40=1#54=1#38=002000.00#55=INTC#60=20031027-14:29:11#10=000#
            </send>
            <!--# expect a reject-->

            <expect>
                8=FIX\.4\.0#9=\d{1,3}#35=3#34=2#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#45=2#58=[\w\s\p{P}]*#10=\d{3}#
            </expect>

            <!--#========================-->

            <!--# end message exchange-->

            <!--#========================-->

            <!--# logout message and response-->

            <send>8=FIX.4.0#9=123#35=5#34=3#49=TW#52=20031027-14:29:11#56=ISLD#10=000#</send>
            <expect>8=FIX\.4\.0#9=\d{1,3}#35=5#34=3#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#10=\d{3}#</expect>
        </transport>
        &ENGINE_DOWN;
    </case>
</cases>
